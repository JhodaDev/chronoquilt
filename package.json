{
  "name": "chronoquilt",
  "version": "1.0.3",
  "description": "Una biblioteca para manejar fechas en javascript",
  "main": "./src/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./src/index.js"
    }
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch ./src --experimental-modules index.js",
    "build:cjs": "swc src -d dist/cjs --config-file swc.cjs.json",
    "build:esm": "swc src -d dist/esm --config-file swc.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "tzdb": "node ./src/instantinlay/zones/tzdb.js",
    "holidays": "node ./src/instantinlay/holidays/2023/co/co.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JhodaDev/datejs.git"
  },
  "keywords": [
    "date",
    "time",
    "chronoquilt",
    "fecha"
  ],
  "author": "JhodaDev",
  "license": "MIT",
  "dependencies": {
    "standard": "^17.1.0"
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100"
  }
}
