{
  "name": "datejs",
  "version": "1.0.0",
  "description": "Una biblioteca para manejar fechas en javascript",
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch ./src --experimental-modules index.js",
    "build": "swc src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JhodaDev/datejs"
  },
  "keywords": [
    "date",
    "time",
    "datejs",
    "fecha"
  ],
  "author": "JhodaDev",
  "license": "MIT",
  "dependencies": {
    "standard": "^17.1.0"
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100"
  }
}
